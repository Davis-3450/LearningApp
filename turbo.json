{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["tsconfig.json", "package-lock.json"],
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "outputs": []
    },
    "type-check": {
      "outputs": []
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"],
      "env": [
        "AGENT_SECRET",
        "REDIS_URL",
        "KV_REST_API_TOKEN",
        "KV_REST_API_URL",
        "KV_REST_API_READ_ONLY_TOKEN",
        "KV_URL"
      ]
    }
  }
}
